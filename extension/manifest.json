{
    "name":"quicktube",
    "description":"tool for automatically speeding up youtube videos",
    "version":"1.0",
    "manifest_version":3,
    "action":{
        "default_popup":"extension.html"
    },
    "host_permissions": [],
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
      },
    "background":{
        "service_worker":"background.js"
    },
    "content_scripts": [
        {
        "js":["content.js"],
        "matches":[
            "https://youtube.com/*",
            "https://www.youtube.com/*"
    ]   }
    ],
    "permissions":["activeTab","scripting","storage"]
   
}